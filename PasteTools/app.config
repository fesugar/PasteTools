      <setting name="CheckBox1Text" serializeAs="String">
        <value>未设置快捷键</value>
      </setting>
      <setting name="CheckBox2Text" serializeAs="String">
        <value>粘贴状态</value>
      </setting>
      <setting name="ComboBox1Text" serializeAs="String">
        <value>default</value>
      </setting>
      <setting name="MaskedTextBox1Text" serializeAs="String">
        <value>120</value>
      </setting>
      <setting name="CheckBox2Checked" serializeAs="String">
        <value>True</value>
      </setting>
      <setting name="CheckBox3Checked" serializeAs="String">
        <value>True</value>
      </setting>
      <setting name="MainPTBackColor" serializeAs="String">
        <value>Gray</value>
      </setting>
      <setting name="TabPage3BackColor" serializeAs="String">
