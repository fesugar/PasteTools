      </setting>
      <setting name="GroupBox1Text" serializeAs="String">
        <value>模式选择</value>
      </setting>
      <setting name="GroupBox2Text" serializeAs="String">
        <value>数据</value>
      </setting>
      <setting name="GroupBox3Text" serializeAs="String">
        <value>杂项</value>
      </setting>
      <setting name="LinkLabelAboutText" serializeAs="String">
        <value>关于...</value>
      </setting>
      <setting name="TitleText" serializeAs="String">
        <value>粘贴工具</value>
      </setting>
      <setting name="LabelTb3ColorText" serializeAs="String">
